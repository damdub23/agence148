        </li>
    {% endif %}
    {% if options.twitter %}
        <li>
            <a href="{{ options.twitter }}" target="_blank" class="x"></a>
        </li>
    {% endif %}
    {% if options.linkedin %}
        <li>
            <a href="{{ options.linkedin }}" target="_blank" class="linkedin"></a>
